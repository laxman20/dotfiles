set ideajoin
set ideamarks
set commentary
set incsearch
set ignorecase
set smartcase

Plug 'vim-scripts/argtextobj.vim'
let g:argtextobj_pairs="[:],(:),<:>"

Plug 'vim-scripts/ReplaceWithRegister'
Plug 'machakann/vim-highlightedyank'

inoremap jk <Esc>
nmap <C-o> <Action>(Back)
nmap <C-i> <Action>(Forward)

set clipboard+=unnamed



